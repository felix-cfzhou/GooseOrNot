{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "no-any": true,
        "max-classes-per-file": [true, 5],
        "jsx-boolean-value": false,
        "jsx-no-lambda": false,
        "only-arrow-functions": false,
        "semicolon": [true, "always"],
        "trailing-comma": [true, {
            "multiline": "always",
            "singleline": "never"
        }],
        "array-type": {
            "options": [
                "generic"
            ]
        },
        "interface-name": {
            "options": [
                "never-prefix"
            ]
        },
        "max-line-length": {
            "options": [
                128
            ]
        }
    }
}
